{
  "name": "openapi-axios-mock-adapter",
  "version": "0.0.3",
  "description": "for axios-mock-adapter",
  "main": "lib/index.js",
  "repository": "https://github.com/tnkgs/openapi-axios-mock-adapter.git",
  "author": "tnkgs <kento@kt-one.jp>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "bin": {
    "generate-axios-mock-adapter": "bin/generate-axios-mock-adapter.js"
  },
  "scripts": {
    "prepublish": "tsc",
    "build": "tsc",
    "clean": "rimraf lib"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@types/js-yaml": "^3.12.3",
    "@types/node": "^14.0.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "commitlint-config-gitmoji": "^1.0.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^9.0.1",
    "openapi-sampler": "^1.0.0-beta.15",
    "tosource": "^1.0.0"
  },
  "peerDependencies": {
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.18.1"
  }
}
